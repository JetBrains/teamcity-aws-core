<?xml version="1.0" encoding="UTF-8"?>
<dsl-extension kind="buildFeature" type="AWS_CREDS_TO_AGENT" generateDslJar="true">
  <class name="ProvideAwsCredentialsToAgent">
    <description>
      Provides AWS Credentials of chosen AWS Connection to agent.
    </description>
  </class>

  <function name="provideAwsCredentialsToAgent">
    <description>
      @see ProvideAwsCredentialsToAgent
    </description>
  </function>

  <params>
    <param name="awsConnectionId" dslName="awsConnectionId" type="string" mandatory="true">
      <description>
        AWS Connection ID.
        @see AwsConnection
      </description>
    </param>

    <param name="awsSessionDuration" dslName="sessionDuration" type="string">
      <description>
        Session duration in minutes
      </description>
    </param>
  </params>
</dsl-extension>