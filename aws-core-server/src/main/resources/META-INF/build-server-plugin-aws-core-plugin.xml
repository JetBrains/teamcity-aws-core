<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-autowire="constructor">

<!--    open-api beans-->
    <bean id="awsConnectionsManagerProvider" class="jetbrains.buildServer.clouds.amazon.connector.featureDevelopment.AwsConnectionsManagerProvider"/>
    <bean id="awsConnectionsManager" factory-bean="awsConnectionsManagerProvider" factory-method="getManager"/>

    <bean id="awsConnectorFactoryProvider" class="jetbrains.buildServer.clouds.amazon.connector.impl.AwsConnectorFactoryProvider"/>
    <bean id="awsConnectorFactory" factory-bean="awsConnectorFactoryProvider" factory-method="getFactory"/>

    <bean id="staticCredsBuilderProvider" class="jetbrains.buildServer.clouds.amazon.connector.impl.staticType.StaticCredsBuilderProvider"/>
    <bean id="staticCredsBuilder" factory-bean="staticCredsBuilderProvider" factory-method="getBuilder"/>
<!--    ^-->

    <bean class="jetbrains.buildServer.clouds.amazon.connector.backwardsCompat.AwsConnectorExtensionRegistar"/>

    <bean class="jetbrains.buildServer.serverSide.oauth.aws.controllers.AvailableAwsConnsController"/>

    <bean class="jetbrains.buildServer.clouds.amazon.connector.connectionTesting.impl.AwsConnectionTesterImpl"/>
    <bean class="jetbrains.buildServer.serverSide.oauth.aws.controllers.AwsTestConnectionController"/>
</beans>