<?xml version="1.0" encoding="UTF-8"?>

<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
    <info>
        <name>aws-core-plugin</name>
        <display-name>AWS Core</display-name>
        <version>@Plugin_Version@</version>
        <description>Core utilities common for TeamCity Amazon-related plugins. Before TeamCity 2019.2 was a part of teamcity-aws-codedeploy-plugin.</description>
        <download-url>https://github.com/JetBrains/teamcity-aws-core-plugin</download-url>
        <vendor>
            <name>JetBrains</name>
            <url>https://www.jetbrains.com</url>
        </vendor>
    </info>
    <deployment use-separate-classloader="true" node-responsibilities-aware="true"/>
</teamcity-plugin>